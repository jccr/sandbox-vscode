{
  "name": "sandbox",
  "displayName": "Sandbox",
  "description": "Instantly test snippets of JS/HTML/CSS using VS Code.",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.47.0"
  },
  "author": {
    "name": "Juan Carlos Corona Romero <c@rlos.email> (https://jccr.dev/)"
  },
  "license": "MIT",
  "homepage": "https://github.com/jccr/sandbox-vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/jccr/sandbox-vscode"
  },
  "bugs": {
    "url": "https://github.com/jccr/sandbox-vscode/issues"
  },
  "categories": [
    "Debuggers",
    "Other"
  ],
  "keywords": [
    "playground",
    "sandbox",
    "scratchpad",
    "repl",
    "javascript"
  ],
  "publisher": "jccr",
  "icon": "assets/icon-256.png",
  "activationEvents": [
    "onFileSystem:sandbox",
    "onCommand:sandbox.newSandbox",
    "onCommand:sandbox.reopenEditors",
    "onCommand:sandbox.closeSandbox"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "New Sandbox",
        "category": "Sandbox",
        "command": "sandbox.newSandbox"
      },
      {
        "title": "Reopen Editors",
        "category": "Sandbox",
        "command": "sandbox.reopenEditors"
      },
      {
        "title": "Close Sandbox",
        "category": "Sandbox",
        "command": "sandbox.closeSandbox"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "jsdom": "15.1.1"
  },
  "devDependencies": {
    "@types/jsdom": "^12.2.4",
    "@types/vscode": "^1.47.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^10.2.0",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
